;--------------------------------------------------
; Constants for DAC8B API's
;--------------------------------------------------

DAC8B_OFF:              equ 00h
DAC8B_LOWPOWER:         equ 01h
DAC8B_MEDPOWER:         equ 02h
DAC8B_HIGHPOWER:        equ 03h

DAC8B_LSB_CR0:          equ 98h
DAC8B_LSB_CR1:          equ 99h
DAC8B_LSB_CR2:          equ 9ah
DAC8B_LSB_CR3:          equ 9bh
DAC8B_MSB_CR0:          equ 88h
DAC8B_MSB_CR1:          equ 89h
DAC8B_MSB_CR2:          equ 8ah
DAC8B_MSB_CR3:          equ 8bh

DAC8B_OffsetBinary:     equ 04h
DAC8B_TwosComplement:   equ 02h
DAC8B_SignAndMagnitude: equ 01h
DAC8B_RawRegister:      equ 00h

DAC8B_DATAFORMAT:       equ 3h
DAC8B_OFFSETBINARY:     equ DAC8B_DATAFORMAT & DAC8B_OffsetBinary
DAC8B_TWOSCOMPLEMENT:   equ DAC8B_DATAFORMAT & DAC8B_TwosComplement
DAC8B_SIGNANDMAGNITUDE: equ DAC8B_DATAFORMAT & DAC8B_SignAndMagnitude

DAC8B_PHASE_Normal:     equ 0
DAC8B_PHASE_Swapped:    equ 1

DAC8B_PHASE_SWAP:       equ DAC8B_PHASE_Normal

IF DAC8B_PHASE_SWAP
    DAC8B_CR0_HIBITS:   equ C0h
ELSE
    DAC8B_CR0_HIBITS:   equ 80h
ENDIF

