include "m8c.inc"


;--------------------------------------------------
; Constants for PWMPaino2 API's.
;--------------------------------------------------

PWMPaino2_CONTROL_REG_START_BIT:           equ 0x01    ; Control register start bit
PWMPaino2_INT_REG:                         equ 0x0e1
PWMPaino2_INT_MASK:                        equ 0x02


;--------------------------------------------------
; Constants for PWMPaino2 user defined values
;--------------------------------------------------

PWMPaino2_PERIOD:                          equ 0x0
PWMPaino2_PULSE_WIDTH:                     equ 0x0


;--------------------------------------------------
; Register Address Constants for PWMPaino2
;--------------------------------------------------

PWMPaino2_COUNTER_LSB_REG:             equ 0x20   ; DR0 Count register
PWMPaino2_COUNTER_MSB_REG:             equ 0x24
PWMPaino2_PERIOD_LSB_REG:              equ 0x21   ; DR1 Period register
PWMPaino2_PERIOD_MSB_REG:              equ 0x25
PWMPaino2_COMPARE_LSB_REG:             equ 0x22   ; DR2 Compare register
PWMPaino2_COMPARE_MSB_REG:             equ 0x26
PWMPaino2_CONTROL_LSB_REG:             equ 0x23   ; Control register
PWMPaino2_CONTROL_MSB_REG:             equ 0x27
PWMPaino2_FUNC_LSB_REG:                equ 0x20   ; Function register
PWMPaino2_FUNC_MSB_REG:                equ 0x24
PWMPaino2_INPUT_LSB_REG:               equ 0x21   ; Input register
PWMPaino2_INPUT_MSB_REG:               equ 0x25
PWMPaino2_OUTPUT_LSB_REG:              equ 0x22   ; Output register
PWMPaino2_OUTPUT_MSB_REG:              equ 0x26


;--------------------------------------------------
; PWMPaino2 Macro 'Functions'
;--------------------------------------------------

   macro PWMPaino2_Start_M
   or    reg[PWMPaino2_CONTROL_LSB_REG],  PWMPaino2_CONTROL_REG_START_BIT
   endm

   macro PWMPaino2_Stop_M
   and   reg[PWMPaino2_CONTROL_LSB_REG], ~PWMPaino2_CONTROL_REG_START_BIT
   endm

   macro PWMPaino2_EnableInt_M
   M8C_EnableIntMask PWMPaino2_INT_REG, PWMPaino2_INT_MASK
   endm

   macro PWMPaino2_DisableInt_M
   M8C_DisableIntMask PWMPaino2_INT_REG, PWMPaino2_INT_MASK
   endm


